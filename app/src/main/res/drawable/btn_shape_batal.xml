<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#E6EAED" />
            <stroke
                android:width="2dp"
                android:color="#E6EAED" />
            <corners
                android:radius="5dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#EEF2F5" />
            <stroke
                android:width="1dp"
                android:color="#EEF2F5" />
            <corners
                android:radius="5dp" />
        </shape>
    </item>
</selector>